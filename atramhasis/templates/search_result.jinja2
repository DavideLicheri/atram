{% extends 'templates/base.jinja2' %}

{% block html_title %}Search{% endblock %}

{% block content %}

    <h2>{{concepts|length}} result(s):</h2>

    <ul>
        {%- for c in concepts %}
         <li>{{ c.id }}: <a href="c/{{ c.id }}">{{ c.label }}</a></li>
        {%- endfor %}
    </ul>

{% endblock %}
